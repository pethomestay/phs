<div class="row">
	<div class="span12">
		<div class="well">
			<h1>Find a homestay</h1>
		</div>
	</div>
</div>
<div class="row">
	<%= render partial: 'search_box' %>
	<div class="span9">
		<table class="table table-striped table-bordered">
			<tbody>
				
			<% @homestays.each do |homestay| %>
				<%= render partial: 'result_row', locals: {homestay: homestay} %>
			<% end %>

			</tbody>
		</table>
	</div>
</div>